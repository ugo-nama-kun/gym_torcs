<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="EVOWRC61" type="template">
	
	<section name="Car">
		<attnum name="initial fuel" min="1.0" max="58.0" unit="l" val="58.0"/>
	</section>
		
	<section name="Front Wing">
		<attnum name="angle" min="0" max="30" unit="deg" val="5"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" min="0" max="80" unit="deg" val="15"/>
	</section>
	
	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-5" max="0" val="-2.96"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0" max="10" val="2.232"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0" max="10" val="1.625"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0" max="10" val="1.086"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0" max="10" val=".856"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0" max="10" val=".69"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0" max="10" val="0.6"/>
			</section>
		</section>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.0" max="1.0" val="0.65"/>
		<attnum name="max pressure" min="100" max="1500000" unit="kPa" val="15000"/>
	</section>
		
	<section name="Front Differential">
		<!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"/>
	</section>
	
	<section name="Rear Differential">
		<!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
	</section>
	
	<section name="Central Differential">
		<attnum name="ratio" min="0" max="10" val="6.75"/>
	</section>
	
	<section name="Front Right Wheel">
		<attnum name="ride height" min="100" max="250" unit="mm" val="100"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
	</section>
	
	<section name="Front Left Wheel">
		<attnum name="ride height" min="100" max="250" unit="mm" val="100"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
	</section>
	
	<section name="Rear Right Wheel">
		<attnum name="ride height" min="100" max="250" unit="mm" val="100"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
	</section>
	
	<section name="Rear Left Wheel">
		<attnum name="ride height" min="100" max="250" unit="mm" val="100"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3"/>
	</section>
	
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="0"/>
	</section>
	
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="0"/>
	</section>
	
	<section name="Front Right Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
		<attnum name="suspension course" min="0" max="1.0" unit="m" val="0.30"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
		<attnum name="packers" min="50" max="100" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="200"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="100"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="50"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	
	<section name="Front Left Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
		<attnum name="suspension course" min="0" max="1.0" unit="m" val="0.30"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
		<attnum name="packers" min="50" max="100" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="200"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="100"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="50"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	
	<section name="Rear Right Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
		<attnum name="suspension course" min="0" max="1.0" unit="m" val="0.30"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
		<attnum name="packers" min="50" max="100" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="200"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="50"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	
	<section name="Rear Left Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
		<attnum name="suspension course" min="0" max="1.0" unit="m" val="0.30"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.5"/>
		<attnum name="packers" min="50" max="100" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="200"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="50"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	
	<section name="Front Right Brake">
		<attnum name="disk diameter" min="100" max="500" unit="mm" val="350"/>
		<attnum name="piston area" min="25" max="50" unit="cm2" val="50"/>
		<attnum name="mu" min="0.25" max="0.5" val="0.45"/>
		<attnum name="inertia" unit="kg.m2" val="0.1241"/>
	</section>
	
	<section name="Front Left Brake">
		<attnum name="disk diameter" min="100" max="500" unit="mm" val="350"/>
		<attnum name="piston area" min="25" max="50" unit="cm2" val="50"/>
		<attnum name="mu" min="0.25" max="0.5" val="0.45"/>
		<attnum name="inertia" unit="kg.m2" val="0.1241"/>
	</section>
	
	<section name="Rear Right Brake">
		<attnum name="disk diameter" min="100" max="500" unit="mm" val="200"/>
		<attnum name="piston area" min="25" max="50" unit="cm2" val="50"/>
		<attnum name="mu" min="0.25" max="0.5" val="0.45"/>
		<attnum name="inertia" unit="kg.m2" val="0.1241"/>
	</section>
	
	<section name="Rear Left Brake">
		<attnum name="disk diameter" min="100" max="500" unit="mm" val="200"/>
		<attnum name="piston area" min="25" max="50" unit="cm2" val="50"/>
		<attnum name="mu" min="0.25" max="0.5" val="0.45"/>
		<attnum name="inertia" unit="kg.m2" val="0.1241"/>
	</section>
	
	
  <section name="berniw private">
    <attnum name="caero" val="1.0"/>
    <attnum name="cfriction" val="1.1"/>
  </section>

</params>



