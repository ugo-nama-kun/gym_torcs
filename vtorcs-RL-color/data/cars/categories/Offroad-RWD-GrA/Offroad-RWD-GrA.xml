    <attnum name="suspension course" unit="m" min="0.1" max="0.8" val="0.8"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" unit="mm" val="0"/>
    <attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="50"/>
    <attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="50"/>
