    <attnum name="mu" min="0.05" max="1.8" val="1.7"/>
    <attnum name="rolling resistance" val="0.00105"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
    <attnum name="stiffness" min="5.0" max="20.0" val="20.0"/>
