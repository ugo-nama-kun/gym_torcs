    <attnum name="piston area" unit="cm2" val="20"/>
    <attnum name="mu" val="0.45"/>
    <attnum name="inertia" unit="kg.m2" val="0.1241"/>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" unit="mm" min="100" max="200" val="200"/>
    <attnum name="piston area" unit="cm2" val="20"/>
    <attnum name="mu" val="0.45"/>
    <attnum name="inertia" unit="kg.m2" val="0.1241"/>
