  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-5" max="0" val="-3.4"/>
        <attnum name="efficiency" val="0.9"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="2.533333"/>
        <attnum name="efficiency" val="1.0"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="1.650000"/>
        <attnum name="efficiency" val="1.0"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.208333"/>
        <attnum name="efficiency" val="1.0"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="0.962963"/>
        <attnum name="efficiency" val="1.0"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="0.766667"/>
        <attnum name="efficiency" val="1.0"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.65"/>
        <attnum name="efficiency" val="1.0"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" val="45"/>
    <attnum name="max steer speed" unit="deg/s" val="120"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"/>
    <attnum name="max pressure" min="100" max="150000" unit="kPa" val="11000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" val="1.31"/>
    <attnum name="roll center height" unit="m" val="0.3"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" val="-1.36"/>
    <attnum name="roll center height" unit="m" val="0.2"/>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"/>
    <attnum name="ratio" min="0" max="15" val="1.0"/>
    <attnum name="efficiency" val="1.0"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="0" max="15" val="1.0"/>
    <attnum name="efficiency" val="1.0"/>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"/>
    <attnum name="min torque bias" unit="%" val="50"/>
    <attnum name="max torque bias" unit="%" val="60"/>
    <attnum name="ratio" min="0" max="10" val="6.0"/>
    <attnum name="efficiency" val="0.9625"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.82"/>
    <attnum name="rim diameter" unit="in" val="18.0"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".38"/>
    <attnum name="inertia" unit="kg.m2" val="1.1200"/>
    <attnum name="mu" val="1.6"/>
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="rolling resistance" val="0.005"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.82"/>
    <attnum name="rim diameter" unit="in" val="18.0"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".38"/>
    <attnum name="inertia" unit="kg.m2" val="1.1200"/>
    <attnum name="mu" val="1.6"/>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="rolling resistance" val="0.005"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.83"/>
    <attnum name="rim diameter" unit="in" val="18.0"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".38"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" val="1.6"/>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="rolling resistance" val="0.005"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.83"/>
    <attnum name="rim diameter" unit="in" val="18.0"/>
    <attnum name="tire width" unit="mm" val="245"/>
    <attnum name="tire height-width ratio" val=".38"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" val="1.6"/>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="rolling resistance" val="0.005"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" val="1.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="5500"/>
    <attnum name="suspension course" unit="m" val="0.25"/>
    <attnum name="bellcrank" min="1" max="2" val="1.5"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
